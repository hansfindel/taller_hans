<h1>Listing alumns</h1>

<table>
  <tr>
    <th>User</th>
    <th>Course</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @alumns.each do |alumn| %>
  <tr>
    <td><%= alumn.user_id %></td>
    <td><%= alumn.course_id %></td>
    <td><%= link_to 'Show', alumn %></td>
    <td><%= link_to 'Edit', edit_alumn_path(alumn) %></td>
    <td><%= link_to 'Destroy', alumn, :confirm=> 'Are you sure?', :method=> :delete %></td>
  </tr>
<% end %>
</table>

